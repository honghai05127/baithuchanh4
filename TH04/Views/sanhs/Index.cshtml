@model IEnumerable<TH04.Models.sanh>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tensanh)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tensanh)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Masanh">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Masanh">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Masanh">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
